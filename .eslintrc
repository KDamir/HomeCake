{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "jsx-a11y",
    "import",
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prefer-stateless-function": 0,
    "global-require": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/href-no-hash": 0,
    "comma-dangle": 0
  }
}